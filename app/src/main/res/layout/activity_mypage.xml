<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/theme_blue">

    <TextView
        android:id="@+id/mypage_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/theme_blue"
        android:layout_marginTop="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="마이페이지"
        android:fontFamily="@font/netmarbleb"
        android:textSize="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:focusableInTouchMode="false"/>

    <EditText
        android:id="@+id/mypage_name"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:background="@drawable/frame_calendar"
        android:layout_marginTop="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/netmarblel"
        android:textSize="18dp"
        android:inputType="textPersonName"
        android:paddingLeft="7dp"
        app:layout_constraintTop_toTopOf="@id/mypage_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>

    <EditText
        android:id="@+id/mypage_email"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:background="@drawable/frame_calendar"
        android:layout_marginTop="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/netmarblel"
        android:textSize="18dp"
        android:inputType="textPersonName"
        android:paddingLeft="7dp"
        app:layout_constraintTop_toTopOf="@id/mypage_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>


    <EditText
        android:id="@+id/mypage_id"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:background="@drawable/frame_calendar"
        android:layout_marginTop="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/netmarblel"
        android:textSize="18dp"
        android:inputType="numberDecimal"
        android:paddingLeft="7dp"
        app:layout_constraintTop_toTopOf="@id/mypage_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>

    <EditText
        android:id="@+id/mypage_phone"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:background="@drawable/frame_calendar"
        android:layout_marginTop="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/netmarblel"
        android:textSize="18dp"
        android:inputType="textPersonName"
        android:paddingLeft="7dp"
        app:layout_constraintTop_toTopOf="@id/mypage_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>

    <EditText
        android:id="@+id/mypage_birthday"
        android:layout_width="236dp"
        android:layout_height="30dp"
        android:background="@drawable/frame_calendar"
        android:layout_marginTop="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/netmarblel"
        android:textSize="18dp"
        android:inputType="date"
        android:paddingLeft="7dp"
        app:layout_constraintTop_toTopOf="@id/mypage_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>


    <EditText
        android:id="@+id/mypage_dorm"
        android:layout_width="116dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="48dp"
        android:background="@drawable/frame_calendar"
        android:paddingLeft="7dp"
        android:fontFamily="@font/netmarblel"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@id/mypage_name"
        app:layout_constraintTop_toTopOf="@id/mypage_birthday"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>

    <EditText
        android:id="@+id/mypage_dorm2"
        android:layout_width="116dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="48dp"
        android:background="@drawable/frame_calendar"
        android:fontFamily="@font/netmarblel"
        android:inputType="numberDecimal"
        android:paddingLeft="7dp"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/mypage_birthday"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/mypage_birthday"
        tools:ignore="MissingConstraints"
        android:focusableInTouchMode="false"/>

    <Button
        android:id="@+id/mypage_save"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/frame_login_btn"
        android:text="수정하기"
        android:textSize="15dp"
        android:fontFamily="@font/netmarbleb"
        android:textColor="@color/white"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toTopOf="@id/mypage_dorm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>

